generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Bakim {
  id            Int      @id @default(autoincrement())
  firmaIsmi     String
  tezgahSeriNo  String
  tezgahSaati   String
  aciklama      String
  musteriIsmi   String
  muhendisIsmi  String
  createdAt     DateTime @default(now())
}
